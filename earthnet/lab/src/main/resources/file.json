{
	"name": "social plan",
	"allowence": -1,
	"vispServiceID": null,
	"rules": 
	[
		{
			"ruleType": "classification",
			"expressionType": "byApplication",
			"expression": "facebook , twitter , snapchat",
			"attributes": 
			{
				"measurement-method": "volume",
				"multiplier": 1
			}
		},

		{
			"ruleType": "classification",
			"expressionType": "byTime",
			"expression": "between 6PM and 6AM",
			"attributes": 
			{
				"multiplier": 1
			}
		},

		{
			"ruleType": "reporting",
			"expressionType": "byVolume",
			"expression": "32",
			"attributes": 
			{
				"unit": "MBytes",
				"eventType": "periodicUpdateEvent",
				"hostURL": "http:\\localhost:8080//service"
			}
		},

		{
			"ruleType": "qos",
			"expressionType": "byTime",
			"expression": "between 6PM and 6AM",
			"attributes": 
			{
				"bandwidth": 5000000
			}
		}
	]
}