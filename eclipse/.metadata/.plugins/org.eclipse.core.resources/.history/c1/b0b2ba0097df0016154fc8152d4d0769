<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/sample-schema1/"
	targetNamespace="http://www.example.org/sample-schema1/">

	<element name="order">
		<complexType>
			<sequence>
				<element name="product" type="string"></element>
				<element name="address">
					<complexType>
						<sequence>
							<element name="name" type="string"></element>
							<element name="line1" type="string"></element>
							<element name="line2" type="string"></element>
							<element name="line3" type="string"></element>
							<element name="city" type="string"></element>
							<element name="state" type="string"></element>
							<element name="country" type="string"></element>
							<element name="postcode" type="string"></element>
						</sequence>
					</complexType>
				</element>
			</sequence>
			<attribute name="orderid" type="string" use="required"></attribute>
		</complexType>
	</element>
</schema>